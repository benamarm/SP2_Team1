<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="630.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AanvragenController">
   <children>
      <TableView fx:id="aanvragen" focusTraversable="false" layoutX="14.0" layoutY="39.0" onMouseClicked="#clearLabel" prefHeight="577.0" prefWidth="662.0">
        <columns>
          <TableColumn fx:id="colId" editable="false" prefWidth="70.0" style="-fx-alignment: CENTER;" text="ID" />
          <TableColumn fx:id="colNaam" editable="false" minWidth="0.0" prefWidth="180.0" text="NAAM" />
            <TableColumn fx:id="colOpleiding" editable="false" prefWidth="210.0" text="OPLEIDING" />
            <TableColumn fx:id="colVan" editable="false" minWidth="0.0" prefWidth="100.0" style="-fx-alignment: CENTER;" text="VAN" />
            <TableColumn fx:id="colTot" editable="false" minWidth="0.0" prefWidth="100.0" style="-fx-alignment: CENTER;" text="TOT" />
        </columns>
      </TableView>
      <Button fx:id="bCheck" layoutX="798.0" layoutY="222.0" mnemonicParsing="false" onAction="#handleCheck" styleClass="otherbtn" text="CHECK" />
      <Label layoutX="698.0" layoutY="146.0" prefHeight="73.0" prefWidth="270.0" text="Druk op onderstaande knop om de geselecteerde aanvragen te bevestigen." textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="lCheck" layoutX="701.0" layoutY="270.0" prefHeight="30.0" prefWidth="270.0" styleClass="centerlabel" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <CheckBox fx:id="cbSelecteerAlles" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleSelecteerAlles" text="Selecteer alles">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
